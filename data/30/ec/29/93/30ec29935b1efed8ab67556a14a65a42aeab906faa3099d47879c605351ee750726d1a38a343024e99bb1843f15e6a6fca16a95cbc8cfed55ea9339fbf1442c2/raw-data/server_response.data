<script type="text/javascript">
  function setCenterSize() {
    var rightPanel = $("#right-component").width();
    // Setting proportional size
    if (rightPanel>10) {
      if (rightPanel<(940+450)) {
        var contentBodyWidth = rightPanel*0.7;
        var contentTableWidth = rightPanel*0.24;
        $("#contentBody").css("width", contentBodyWidth + "px");
        $("#contentTable").css("width", contentTableWidth + "px");
      } else {
        // Setting default values
        $("#contentBody").css("width", 940 + "px");
        $("#contentTable").css("width", 450 + "px");
      }
    }
    
    // Take in consideration negative values
    if (($(".panel-body").height()-80)>0) {
      if (($(".panel-body").height()-80)<$("#contentAnchors").height()) {
        $("#contentAnchorScroll").css("height", ($(".panel-body").height()-80) + "px");
        $("#contentAnchorScroll").css("overflow-y", "auto");  
      }  
    }
  }

  $(document).ready(
      function() {
        $('div.split-pane').splitPane();

        // -3 is probably caused by pixels borders
        // do not remove $(".panel-breadcrumb").height()
        var height = $(window).height() - $("footer").height() - $("nav").height() - $(".panel-breadcrumb").height() - 3;
        $(".panel-body").css("height", height + "px");

        $('.tree').treegrid();
        
        // Setting center size
        setCenterSize();
        
        // Check control panel visible with cookie
        if (Cookies.get('kCenterControlTable')) {
          hideContentTable();
        } else {
          showContentTable();
        }        
        
          // Case tags only include h1 the size will be 1, then hide table of contents
          $("#contentTable").hide();
        
        // -3 is probably caused by pixels borders
        $(window).resize(
            function() {
              var height = $(window).height() - $("footer").height() - $("nav").height() - $(".panel-breadcrumb").height() - 3;
              $(".panel-body").css("height", height + "px");
              // Setting center size
              setCenterSize();
            });
        
        var contents = new Bloodhound({
            sufficient: 10,
            datumTokenizer: Bloodhound.tokenizers.obj.whitespace('value'),
            queryTokenizer: Bloodhound.tokenizers.whitespace,
            remote: {
              url: '/kcenter/search/docSearch?docId=29&query=%QUERY"/>',              
              wildcard: '%QUERY'
            },
            limit: 10
          });
      
          $('#inputTypeahead').typeahead({
                minLength: 1,
              highlight: true
            },
            {
              name: 'contents',
              display: 'title',
              source: contents.ttAdapter(),
              templates: {
                 suggestion: Handlebars.compile('<p><a href="{{name}}" style="white-space: nowrap;">{{title}}</a></p>')
              }
          });
          // Initialize typeahead
          contents.initialize();
      });
  
  function showContentTable() {
    $("#contentAnchorScroll").show();
    $("#hideContentTable").show();
    $("#showContentTable").hide();
    Cookies.remove('kCenterControlTable');
  }
  
  function hideContentTable() {
    Cookies.set('kCenterControlTable', 'hidden');
    $("#contentAnchorScroll").hide();
    $("#hideContentTable").hide();
    $("#showContentTable").show();
  }
</script>
