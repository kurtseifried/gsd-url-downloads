&gt;&gt; &gt; +*@INTERN Cil
&gt;&gt; &gt;  */*@INTERN i
&gt;&gt; &gt;  EOF
&gt;&gt; &gt;      chmod 644 /etc/krb5kdc/kadm5.acl
&gt;&gt; 
