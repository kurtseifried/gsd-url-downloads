<!-- CUSTOM CODE -->
<script type="text/javascript">

var email 				= "";
var emailInput 			= document.querySelectorAll(".email-input");
var subscribeBtn 		= document.querySelectorAll(".subscribe-btn");
var subscriptionArea 	= document.querySelectorAll(".subscription-area");
var loaders 			= document.querySelectorAll(".loader");
var thankYouArea 		= document.querySelectorAll(".thank-you-area");
var errorMsgArea 		= document.querySelectorAll(".error-msg-area");
var header 				= document.querySelector(".archive header.page-header");
var featuredParagraph 	= document.querySelector("#featured-blog .tzwb-excerpt");
var authorDate 			= document.querySelector("#featured-blog .tzwb-entry-meta.entry-meta");

// 	Places author/date in specified place
if (featuredParagraph && authorDate)
	featuredParagraph.insertBefore(authorDate, featuredParagraph.childNodes[0]);
	
if (header) {
	document.getElementById("secondary").style.paddingTop = header.clientHeight + 50 + "px";
}

for (var i = emailInput.length - 1; i >= 0; i--) {
	emailInput[i].addEventListener("keyup", handleEmailInput, false);
}

for (var i = subscribeBtn.length - 1; i >= 0; i--) {
	subscribeBtn[i].addEventListener("click", handleSubscription, false);
}


function handleSubscription() {

	if (emailIsValid(email)) {
		// Begin UI toggles
		toggleElements(subscriptionArea, false);
		toggleElements(loaders, true);

		// Get frequency (if it exists)
		if (document.querySelector('input[name="frequency"]')) {
			var frequency = document.querySelector('input[name="frequency"]:checked').value;
		}
		
		var url = "/custom-api/Marketo.php?email=" + email + "&frequency=" + (frequency ? frequency : "daily");
		
		handleSubmit(url);
	}
	else {
		for (var i = errorMsgArea.length - 1; i >= 0; i--) {
			errorMsgArea[i].style.visibility = "visible";
		}
	}
}


function handleSubmit(url) {
	// GET request
	var xhr = new XMLHttpRequest();
	xhr.open("GET", url, true);
	xhr.setRequestHeader('X-Requested-With', 'XMLHttpRequest');
	xhr.onload = function() {
		if (xhr.status >= 200 && xhr.status < 300) {
			
			var response = JSON.parse(xhr.responseText);

			if (response.status === true) {
				// Show a thank you confirmation after success
				toggleElements(loaders, false);
				toggleElements(thankYouArea, true);
			}
			else if (response.status === "error") {
				window.alert(response.details);
			}
			else {
				window.alert("There's a server error please try again later.");
			}
		} 
		else {
			// Rejected
			console.log(xhr.responseText);
		}
	};
	xhr.onerror = function() {
		window.alert("There's a network error. Please try again later.");
	};

	xhr.send();
}


function handleEmailInput(e) {

	email 						= e.target.value;

	if (document.getElementById("jumbotronEmailInput")) {
		document.getElementById("jumbotronEmailInput").value 	= email;
		document.getElementById("sidebarEmailInput").value 		= email;
	}

	// Reset error message
	for (var i = errorMsgArea.length - 1; i >= 0; i--) {
		errorMsgArea[i].style.visibility = "hidden";
	}
}


function emailIsValid(email) {
	var pattern = new RegExp(/^((([a-z]|\d|[!#\$%&'\*\+\-\/=\?\^_`{\|}~]|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])+(\.([a-z]|\d|[!#\$%&'\*\+\-\/=\?\^_`{\|}~]|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])+)*)|((\x22)((((\x20|\x09)*(\x0d\x0a))?(\x20|\x09)+)?(([\x01-\x08\x0b\x0c\x0e-\x1f\x7f]|\x21|[\x23-\x5b]|[\x5d-\x7e]|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])|(\\([\x01-\x09\x0b\x0c\x0d-\x7f]|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF]))))*(((\x20|\x09)*(\x0d\x0a))?(\x20|\x09)+)?(\x22)))@((([a-z]|\d|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])|(([a-z]|\d|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])([a-z]|\d|-|\.|_|~|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])*([a-z]|\d|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])))\.)+(([a-z]|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])|(([a-z]|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])([a-z]|\d|-|\.|_|~|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])*([a-z]|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])))\.?$/i);

	if (email && pattern.test(email)) {
		return true;
	}
	return false;
}


function toggleElements(elements, toggle) {
	for (var i = elements.length - 1; i >= 0; i--) {
		elements[i].style.display = toggle ? "block" : "none";
	}
}


// Make the Barracuda logo a link (in the corner)
document.querySelector(".site-branding").onclick = function() {
	window.location.href = document.querySelector(".site-branding a").href;
};


// 	Fixes featured image size
var featuredImg = document.querySelector("#featured-blog img");
if (featuredImg) {
	featuredImg.removeAttribute("srcset");
	featuredImg.src = featuredImg.src.replace("-80x80", "");	
}
</script>





