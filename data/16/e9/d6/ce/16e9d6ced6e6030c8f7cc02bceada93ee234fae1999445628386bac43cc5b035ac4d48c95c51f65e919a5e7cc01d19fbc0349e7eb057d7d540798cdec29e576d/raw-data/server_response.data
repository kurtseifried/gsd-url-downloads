  (function($){
    $(document).ready(function(){
      // open nav
      $(document).on("click",".nav-link.without-dropdown",function() {
        if($(window).width() <= 1199){
          if($(this).children('span.image-icon').hasClass('Home')){
              window.location.href="/";
              return;
          }else{
            $(".nav-wrapper").addClass("open-nav-width");
            $(".nav-link.without-dropdown").hide();
            $(".nav-link.with-dropdown").show();
            $(this).next().children(".nav-dropdown-icon.main.expand").trigger('click');
            $("#lia-body").addClass("push-body-right");
          }
        }
      }); 
      // first level
      $(document).on("click",".nav-dropdown-icon.main.expand",function(e) {
        $(".first_level").hide();
        $(".nav-dropdown-icon.main").attr("class","nav-dropdown-icon main expand"); 
        $(".nav-dropdown-icon.main").text("[+]");
        $(this).parents(".nav_list").find(".first_level").show();
        $(this).text("[-]");
        $(this).attr("class","nav-dropdown-icon main collapse");   
        hideSecondLevel();     
        e.preventDefault(); 
      });
      $(document).on("click",".nav-dropdown-icon.main.collapse",function(e) {
        $(this).parents(".nav_list").find(".first_level").hide();
        $(this).text("[+]");
        $(this).attr("class","nav-dropdown-icon main expand");
        hideSecondLevel();
        e.preventDefault();
      });    
      // second level 
      $(document).on("click",".nav-dropdown-icon.second.expand",function(e) {
      
        if($(window).width()<= 1199 ){
          $(".third-level-wrapper").hide();
          $(".fa-angle-left").attr("class","fa fa-angle-right"); 
          $(this).parents('.second_level').find(".third-level-wrapper").show();
          $(this).find(".fa-angle-right").attr("class","fa fa-angle-left"); 
          $(".nav-dropdown-icon.second").attr("class","nav-dropdown-icon second expand");
          $(this).attr("class","nav-dropdown-icon second collapse");
          e.preventDefault();
        }
      });
      $(document).on("click",".nav-dropdown-icon.second.collapse",function(e) {
        $(this).parents('.second_level').find(".third-level-wrapper").hide();
        $(this).find(".fa-angle-left").attr("class","fa fa-angle-right");
        $(this).attr("class","nav-dropdown-icon second expand");
        e.preventDefault();
      });  

      //close nav on outside click
      $(document).mouseup(function(e){
        var container = $(".nav-container");
        
        if (!container.is(e.target) && container.has(e.target).length === 0) 
        {
          $(".nav-link.with-dropdown").hide();
            //$(".nav-link.without-dropdown.mobile").show(); 
            $(".nav-link.without-dropdown.home").show(); 
          
          //$(".nav-link.without-dropdown").addClass("mobile"); 
          
          $(".nav_list .first_level").hide();    
           console.log('if');
           $("#lia-body").removeClass("push-body-right");
           $("#lia-body").removeClass("body-mobile-padding"); 
          if ($(window).width < 1199){
            console.log("less than 1199");
            $(".nav-hamburger").addClass("show-ham");
            $(".nav-container.mobile").show();
            $(".nav-wrapper").removeClass("open-nav-width");
          }
          else{
            console.log("less than 1199 else");
            $(".nav-hamburger").removeClass("hide-ham");
            $(".nav-container.mobile").hide();
            $(".nav-wrapper").removeClass("open-nav-width");            
          }
        
      }
      });

      $(window).on('resize', function(){
        var win = $(this); //this = window
        if (win.width() <= 1199) { 
          if($('body').hasClass('body-mobile-padding')){
            return;  
          }else{
            $(".nav-hamburger").addClass("show-ham");
            //$(".nav-container.mobile").show();
            $(".nav-container").addClass('mobile');
            $(".nav-container").hide();
            $("body").removeClass('body-mobile-padding');
            $("body").removeClass('push-body-right');
          }
        }
        else{
          console.log('elsevbbv');
          $(".nav-link.mobile").hide();
          $(".nav-hamburger").removeClass("hide-ham");
          $(".nav-container.mobile").hide();
          $("#lia-body").removeClass("push-body-right");
          $("#lia-body").removeClass("body-mobile-padding"); 
          mainArrowCollapse();
          hideAll();
          hideSecondLevel();
          navDesktopMode();
          $(".nav-wrapper").removeClass("open-nav-width");
        }
      });

      // open nav from hamburger
      $(document).on("click",".nav-hamburger",function() {
        $(".nav-container").show();
        $(".nav-container").addClass("mobile");
        $("#lia-body").addClass("body-mobile-padding");
        //$(".nav-link.without-dropdown.mobile").show();
        $(this).addClass("hide-ham");
      });

      function mainArrowCollapse(){
        $(".nav-link.with-dropdown").hide();
        $(".nav-link.without-dropdown").show();
        $(".nav-link.without-dropdown.mobile").hide();
        $(".nav_list .first_level").hide();        
      }

      function hideAll(){
        $(".nav-dropdown-icon.main").attr("class","nav-dropdown-icon main expand");
        $(".nav-dropdown-icon.main").text("[+]");
        $(".nav-dropdown-icon.second").attr("class","nav-dropdown-icon second expand");
        $(".fa.fa-angle-left").attr("class","fa fa-angle-right");
        $(".third-level-wrapper").hide();
      }
      function hideSecondLevel(){
        $(".nav-dropdown-icon.second.collapse").attr("class","nav-dropdown-icon second expand");
        $(".fa.fa-angle-left").attr("class","fa fa-angle-right");
        $(".third-level-wrapper").hide();
      }        

      navDesktopMode();
      // mouseover events
      function navDesktopMode(){
        if($(window).width() > 1199){
          $(".nav-container").removeClass('mobile');
          $(".nav-container").show();
          //$(".nav-link.without-dropdown.mobile").hide();
          $('.nav_list').mouseover(function(){
            $(this).children('.first_level').show();
          });
          $('.second_level').mouseover(function(){
            var scrollposition = $(window).scrollTop();
            var elementposition = $(this).offset().top;
            var finalelementposition = elementposition-scrollposition;
            $(this).children('.third-level-wrapper').css('top',finalelementposition);
            $(this).children(".third-level-wrapper").show();
          })
        }
      }
    });

    
  })(LITHIUM.jQuery);

