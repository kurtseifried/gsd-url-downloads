                <ul ng-if="items.hasDropdown && sso" class="dropdown-menu dropdown-menu-right" aria-labelledby="{{items.dropdownId}}">
                    <li ng-repeat="dditem in items.dropdownList">
                        <a target="{{items.newTab}}" class="dropdown-item" href="{{dditem.url}}">{{dditem.title}}</a>
                    </li>
                </ul>
			</li>
		</ul>
	</nav>
