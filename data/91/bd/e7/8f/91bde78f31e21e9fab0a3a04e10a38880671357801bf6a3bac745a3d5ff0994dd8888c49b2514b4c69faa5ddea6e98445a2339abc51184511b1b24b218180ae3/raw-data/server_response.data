RewriteRule ^blog/([a-zA-Z_-]+)(-[<span class="hljs-number">0</span><span class="hljs-number">-9</span>]+)?/.+\.jpg$ %{ENV:REWRITEBASE}modules/smartblog/images/$<span class="hljs-number">1</span>$<span class="hljs-number">2.</span>jpg [L]</pre> </div>
