	transform:scale(1.05);
}

.showcol .elementor-image::before{
	transition:1s;
	content:"";
	position:absolute;
	width:100%;
	height:0%;
	bottom:0;
	right:0;
	opacity:0.8;
	background:#fff;
	border-radius:10px;
	z-index:9;
}
.showcol:hover .elementor-image::before
{
	height:100%;
	width:100%;
}
.showcontainer {
	transition:1s;
}
.showcol:hover .showcontainer{
	filter:blur(0px);
}
.showcontent{
    transition:0.5s;
    opacity: 0;
