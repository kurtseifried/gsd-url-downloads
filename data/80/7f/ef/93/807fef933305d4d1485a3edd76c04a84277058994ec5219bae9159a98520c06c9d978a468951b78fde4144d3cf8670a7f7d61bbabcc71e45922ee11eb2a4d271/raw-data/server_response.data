  <tr style="border:solid black 1px">
    <th style="border:solid black 1px"> Product Name</th>
    <th style="border:solid black 1px">Default Username</th> 
    <th style="border:solid black 1px">Default Password</th>
  </tr>
  <tr style="border:solid black 1px">
